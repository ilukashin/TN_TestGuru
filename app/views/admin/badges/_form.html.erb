<%= form_with model: [:admin, @badge], local: true do |f| %>
<%= render 'shared/errors', resource: @test %>
  <div class="form-group">
    <p>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>
    </p>
  </div>

  <div class="form-group">
    <p>
      <%= f.label :logo %>
      <%= f.text_field :logo, class: 'form-control' %>
    </p>
  </div>

  <div class="form-group">
    <p>
      <%= f.label :rule %>
      <%= f.text_field :rule, class: 'form-control' %>
    </p>
  </div>

  <div class="form-group">
    <p>
      <%= f.label :value %>
      <%= f.text_field :value, class: 'form-control' %>
    </p>
  </div>

  <div class="form-group">
    <p><%= f.submit class: 'btn btn-success' %></p>
  </div>

<% end %>
